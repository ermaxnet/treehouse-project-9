{
  "name": "treehouse-project-9",
  "description": "Build awesome Flick Gallery using ReactJS",
  "version": "1.0.0",
  "author": "Max Eremin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ermaxnet/treehouse-project-9.git"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-assets": "gulp build:assets",
    "watch-css": "gulp watch:scss",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p build-assets watch-css start-js",
    "build": "npm-run-all build-assets build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "paths": {
    "src": {
      "scss": "./assets/scss/",
      "images": "./assets/images/"
    },
    "dist": {
      "css": "./public/assets/css/",
      "images": "./public/assets/images/"
    },
    "build": {
      "css": "./assets/css/"
    },
    "compass": "./config.rb"
  },
  "globs": {
    "distCss": [
      "./assets/css/*.css"
    ]
  },
  "vars": {
    "scssName": "styles.scss"
  },
  "devDependencies": {
    "del": "^3.0.0",
    "fancy-log": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-compass": "^2.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-header": "^2.0.5",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-print": "^5.0.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svgmin": "^1.2.4",
    "moment": "^2.22.0",
    "npm-run-all": "^4.1.2",
    "run-sequence": "^2.2.1"
  }
}
